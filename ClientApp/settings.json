{
  "breadcrumbs.enabled": true,
  // "css.lint.emptyRules": "ignore",
  "editor.fontFamily": "Fira Code, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontWeight": "400",
  "editor.fontSize": 14,
  "editor.lineHeight": 22,
  "editor.minimap.enabled": true,
  // "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "none",
  // "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.suggestSelection": "first",
  "explorer.autoReveal": true,
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.rts2*": true,
    // "**/node_modules": true,
    "**/.bundles": true,
    "**/.build": true,
    "**/cosmos-export": true,
    "**/.awcache": true,
    "**/.nyc_output": true
  },
  "files.associations": {
    "*.js": "javascriptreact",
    "*.lock": "log",
    ".envrc": "dotenv"
  },
  "git.autofetch": true,
  "javascript.suggest.completeFunctionCalls": true,
  "merge-conflict.codeLens.enabled": true,
  "php.validate.executablePath": "C:\\php\\php.exe",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.build": true,
    "**/.zefir": true
  },
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "Consolas, monospace",
  // "terminal.integrated.fontFamily": "DejaVu, Sans Mono for Powerline, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "terminal.integrated.windowsEnableConpty": false,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeight": "500",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.font": 1.2,
  "todohighlight.isEnable": true,
  "todohighlight.keywords": [
    {
      "text": "TODO:",
      "backgroundColor": "#03A9F4"
    },
    {
      "text": "FIXME:",
      "backgroundColor": "#FF7043"
    }
  ],
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
  "window.titleBarStyle": "custom",
  // "window.zoomLevel": -2,
  "window.zoomLevel": 0,
  "workbench.editor.focusRecentEditorAfterClose": true,
  "workbench.activityBar.visible": true,
  "workbench.editor.showTabs": false,
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "zenMode.hideStatusBar": true,
  "zenMode.hideTabs": true,
  "breadcrumbs.symbolPath": "off",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "gitlens.mode.active": "review",
  "gitlens.views.repositories.files.layout": "list",
  "git.confirmSync": false,
  "workbench.preferredLightColorTheme": "Night Owl Light",
  "workbench.preferredDarkColorTheme": "Monokai Pro (Filter Octagon)",
  // "window.autoDetectColorScheme": true,
  "sync.gist": "f06d4484c27dba07d4e350274f248a5f",
  "gitlens.codeLens.enabled": false,
  "terminal.integrated.fastScrollSensitivity": 6,
  "output.smartScroll.enabled": false,
  "eslint.alwaysShowStatus": true,
  "eslint.codeAction.disableRuleComment": {
    "enable": true
  },
  "eslint.format.enable": true,
  "eslint.validate": [],
  "eslint.options": {},
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html"
  ],
  "typescript.tsserver.watchOptions": {
    "watchFile": "useFsEvents"
  },
  "editor.formatOnSave": true,
  "todo-tree.tree.showScanModeButton": false,
  "workbench.sideBar.location": "left",
  "workbench.colorTheme": "Monokai Pro (Filter Octagon)",
  "workbench.iconTheme": "Monokai Pro (Filter Octagon) Icons"
}
